<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clickhotelmanagementsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CLiCKHotelManagementSystem"
        android:fullBackupContent="@xml/backup_descriptor">

        <activity android:name=".LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.tasks.MainActivity" />
        <activity android:name="com.example.tasks.StaffActivity" />
        <activity android:name=".Manager.MainPageManager" />
        <activity android:name=".Manager.Reservations" />
        <activity android:name=".Manager.AccountManager" />
        <activity android:name=".Staff.MainPageStaff" />
        <activity android:name=".DashboardActivity"
            android:theme="@style/Theme.CLiCKHotelManagementSystem.NoActionBar"/>
        <activity android:name=".ItemActivity"
            android:theme="@style/Theme.CLiCKHotelManagementSystem.NoActionBar"
            android:label="@string/app_name"/>


    </application>

</manifest>